{
    "name": "josh/livewire-autocomplete",
    "description": "An autocomplete dropdown for Livewire",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Hanley",
            "email": "josh@hitsystems.com.au"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "LivewireAutocomplete\\": "src/LivewireAutocomplete/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LivewireAutocomplete\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LivewireAutocomplete\\LivewireAutocompleteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "joshhanley/livewire-dusk-testbench": "^0.1.0"
    }
}
